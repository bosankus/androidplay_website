{
  "hosting": {
    "public": "index",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],

    "appAssociation": "AUTO",
    "rewrites": [
      { "source": "/ahead/**", "dynamicLinks": true },
      { "source": "/finrec/**", "dynamicLinks": true },
      { "source": "!/**", "destination": "/index.html" }
    ]
  }
}
